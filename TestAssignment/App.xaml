<Application x:Class="TestAssignment.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TestAssignment"
             xmlns:vm="clr-namespace:TestAssignment.ViewModels"
             StartupUri="Views/Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Infrastructure/Commands/Commands.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <vm:ViewModelsLocator x:Key="Locator"/>
        </ResourceDictionary>
         
    </Application.Resources>
</Application>
